#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,min=1,sum=0;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	    {
	    cin>>a[i];
	    sum+=a[i];
	    if(sum<0&&(abs(sum)>=min))
	    min=abs(sum)+1;
	    }
	    cout<<min<<endl;
	}
	return 0;
}
